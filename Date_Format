def format_date(date=str(11/11/1111)):
  
  if len(str(date)) == 10 and date[2] == '/' and date[5] == '/':
    return "{}{}{}".format(date[6:],date[3:5],date[0:2])
  else:
    return "Invalid Format"
  


prac_date = str(input("Enter a date in the following format: ex) 10/20/2018"))

print(format_date(prac_date))
